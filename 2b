#include<iostream>
using namespace std;

class sample
{
 int f=0, s=1, t, n, i;
public:
void fibo()
{
       cout<<"Enter the no. of sequence";
         cin>>n;
         cout<<"Fibonacci sereis :";
         cout<<f<<s;
         for(i=2; i<=n; i++){
            t=f+s;
            cout<<t;
            f=s;
            s=t;
         }    
}     
};

int main(){
   sample o1;
   o1.fibo();

    return 0;
}
