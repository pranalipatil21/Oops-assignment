#include<iostream>
using namespace std;
class student{
int roll,age;
string name,branch,year;
public:
void read();
void show();
student();
student(int,string);

};

void student::read(){
    cout<<"Enter the student data base";
    cin>>roll>>name>>branch>>age>>year;
}
void student::show(){
    cout<<roll<<endl<<name<<endl<<branch<<endl<<age<<endl<<year<<endl;
}
student :: student(){
    roll=10;
    name="pccoe";
    branch="comp";
    age=18;
    year="FY";
}
int main(){
    student s1;
    s1.read();
    s1.show();
    
    return 0;
}
